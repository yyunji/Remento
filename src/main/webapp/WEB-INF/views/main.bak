<script type="text/javascript">
$(function(){	
    var totalScroll = $(document).scrollTop(),
    	$header = $('#header').outerHeight(),
    	lastScroll = 0;
    
    
    $(window).scroll(function( event ) {
    	
        var scrollValue = $(document).scrollTop();
        
    	if ( scrollValue > $header ) {
    		$('#header').addClass("sticky-header");
    	} else if ( scrollValue == 0 ) {
    		$('#header').removeClass("sticky-header");
    	}
    	
    	if ( scrollValue > 0 ) {
    		$(".first-section-arrow").addClass("hide");
    	} else {
    		$(".first-section-arrow").removeClass("hide");
    	}


        if (scrollValue > $header){$('#header').addClass('sticky-header-up');} 
        else {$('#header').removeClass('sticky-header-up');}

        if (scrollValue > totalScroll){$('#header').removeClass('sticky-header-down');} 
        else {$('#header').addClass('sticky-header-down');}				

        totalScroll = $(document).scrollTop();	
     });
});

